# V2 selection and polling settings
V2_TOP_USERS=10
V2_CANDIDATE_LIMIT=300
V2_MIN_WIN_RATE=80
V2_MIN_TRADES_PER_DAY=1
V2_MAX_TRADES_PER_DAY=25
V2_TRADE_POLL_SECONDS=60
V2_TRADE_PAGE_SIZE=200
V2_TRADE_MAX_PAGES_PER_POLL=10
